{# Page tabs #}{# Add page header for tabs with {{ init_tabs() }} in your page header #}{% block page_tab_head %}	{% import "js_helpers.html.twig" as js %}	{{ js.page_tabs(default_tab) }}{% endblock %}{# Begin an existing tab with {% tab a_tab_name %} #}{# Create/overwrite one with {% tab a_tab_name(a_tab_id,a_tab_title) %} #}{% block page_tab_begin %}	<div class="multi-part" title="{{ title }}" id="{{ id }}">{% endblock %}{# End a tab with {% endtab %} #}{% block page_tab_end %}	</div>{% endblock %}{# Add fake tab with {{ faketab(a_tab_name) }} #}{# Create/overwrite one with {{ faketab(a_tab_name,a_tab_id,a_tab_title,a_tab_url) }} #}{% block page_tab_fake %}	<a class="multi-part" href="{{ url }}" title="{{ title }}">{{ title }}</a>{% endblock %}